{
    "name" : "Weather extension",
    "description" : "displays weather at different places",
    "version" : "0.0.1",
    "manifest_version" : 3,
    "action" : {
        "default_popup" : "./popup.html",
        "default_icon" : "./icon_128.png"
    },
    "icons": {
        "16" : "./icon_16.png",
        "48" : "./icon_48.png",
        "128" :"./icon_128.png"
    },
    "permissions":["activeTab","geolocation"]
}